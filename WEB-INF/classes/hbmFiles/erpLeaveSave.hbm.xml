<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.leave.dto.ErpAvailableLeaveSaveDTO" table="ERP_LEAVE_REQUEST_DETAILS">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">ERPLEAVE_DETAILS_SEQ</param>
			</generator>
		</id>
		<property name="requestId" column="REQUEST_ID" />
		<property name="sfID" column="SFID" />
		<property name="designation" column="DESIGNATION_ID" />
		<property name="contactNo" column="CONTACT_NUMBER" />
		<property name="leaveType" column="LEAVE_TYPE_ID" />
		<property name="fromDate" column="FROM_DATE" />
		<property name="toDate" column="TO_DATE" />
		<property name="noOfDays" column="NO_OF_DAYS" />
		<property name="reason" column="REASON" />
		<property name="address" column="ADDRESS" />
		<property name="requestedBy" column="REQUESTED_BY" />
		<property name="applyDate" column="REQUESTED_DATE" />
		<property name="status" column="STATUS" />
		<property name="reasonCancellation" column="REASON_LEAVECANCELLATION" />
		<property name="cancelRequestId" column="CANCEL_REQUESTID" />
		<property name="leaveStatus" column="LEAVE_STATUS" />
		
		
		<property name="prescriptionYorN" column="PRESCRIPTION_AVAIL_NOT" />
		<property name="prescriptiondoc" column="PRESCRIPTION_COPY" />


		<!-- <property name="" /> -->

		
	</class>
</hibernate-mapping>