<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.ltc.dto.LTCWaterFamilyDTO" table="LTC_WATER_FAMILYDETAILS">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">ADDRESS_DETAILS_SEQ</param>
			</generator>
		</id>
		<property name="sfID" column="SFID" />
		<property name="requestId" column="REQUESTID" />
		<property name="ltcYear" column="LTCYEAR" />
		<property name="familyMemberId" column="FAMILY_MEMBER_ID" />
	</class>
</hibernate-mapping>