<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.leave.dto.ErpAvailableLeavesDTO" table="ERP_AVAILABLE_LEAVES">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">ERPLEAVE_DETAILS_SEQ</param>
			</generator>
		</id>
		<property name="sfID" column="SFID" />
		<property name="annualLeaves" column="ANNUAL_LEAVES" />
		<property name="sickLeaves" column="SICK_LEAVES" />
		<property name="maternityLeaves" column="MATERNITY_LEAVES" />
		<property name="peternityLeaves" column="PETERNITY_LEAVES" />
		
		
	</class>
</hibernate-mapping>