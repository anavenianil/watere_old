<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.beans.nominee.NomineeBean" table="nominee_details">
		<id name="id" column="id"></id>
		<property name="sfid" column="sfid" />
		<property name="nomineeName" column="nominee" />
		<property name="percentage" column="percentage" />
		<property name="creationDate" column="creation_date" update="false"/>
		<property name="status" column="status" />
		<property name="remarks" column="remarks" />
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="dateOfNominate" column="date_of_nominate" />
		<property name="familyID" column="family_member_id" />
		<property name="nomineeTypeId" column="nominee_type_id" />
		<property name="inconsistencyParentId" column="incontangensy_parent" />
	</class>		
</hibernate-mapping>