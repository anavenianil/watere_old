<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.beans.test.TestMasterDTO" table="TEST_MASTER">
		<id name="id" column="ID" >
			<generator class="sequence">
				<param name="sequence">test_master_id</param>
			</generator>
		</id>
		<property name="name" column="NAME" />
		<property name="status" column="STATUS" />
		<property name="creationTime" column="creation_date" />
		<property name="lastModifiedTime" column="last_modified_date" />
	</class>
</hibernate-mapping>