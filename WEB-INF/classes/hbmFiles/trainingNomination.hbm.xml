<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.hrdg.training.request.beans.dto.TrainingNominationDTO" table="HRDG_TRAINING_NOMINATION_MST">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">HRDG_TRAINING_NOMI_MST_SEQ</param>
			</generator>
		</id>
		<property name="cirId" column="cir_id" />
		<property name="requestId" column="request_id" />
		<property name="nomineeSfid" column="nominee_sfid" />
		<property name="sfid" column="sfid" />
		<property name="offlineFlag" column="offline_flag" />
		<property name="currentAssignment" column="current_assignment" />
		<property name="relevance" column="relevance" />
		<property name="lastAttendedCourse" column="last_attended_course" />
		<property name="nominationDate" column="nomination_date" />
		
		
		<property name="status" column="status" />
		<property name="creationDate" column="creation_Date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />


	</class>
</hibernate-mapping>



