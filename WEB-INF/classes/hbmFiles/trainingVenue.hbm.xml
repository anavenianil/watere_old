<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.hrdg.training.beans.dto.TrainingVenueDTO" table="HRDG_TRAINING_VENUE_DETAILS">
		<id name="id" column="training_venue_id">
			<generator class="sequence">
				<param name="sequence">HRDG_TRAINING_VENUE_DET_SEQ</param>
			</generator>
		</id>
		<property name="cityId" column="city_id" />
		<property name="address" column="address" />
		<property name="phone" column="phone" />
		<property name="fax" column="fax" />
		<property name="email" column="email" />
		<property name="headOfficeFlag" column="head_office_flag" />
		<property name="ddFlag" column="dd_flag" />
		<property name="ddAddress" column="dd_address" />
		<property name="payableAt" column="payable_at" />
		<property name="trainingInistituteId" column="training_inistitute_id"/>
		<property name="status" column="status" />
		<property name="creationDate" column="creation_Date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />


	</class>
</hibernate-mapping>

