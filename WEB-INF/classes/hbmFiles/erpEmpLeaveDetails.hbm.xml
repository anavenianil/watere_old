<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.leave.dto.ErpEmpLeavesDTO" table="ERP_EMPLOEE_LEAVES">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">ERPLEAVE_SEQ</param>
			</generator>
		</id>
	<property name="sfID" column="SFID" />
	<property name="leaveType" column="LEAVE_TYPE" />
	<property name="leaveCode" column="LEAVE_CODE" />
	<property name="status" column="STATUS" />
	<property name="noOfDays" column="NO_OF_DAYS" />
		
		
		
		
		
	</class>
</hibernate-mapping>