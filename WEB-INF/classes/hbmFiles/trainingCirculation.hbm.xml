<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.hrdg.training.beans.dto.TrainingCirculationDTO" table="HRDG_TRAINING_CIR_MASTER">
		<id name="id" column="cir_id">
			<generator class="sequence">
				<param name="sequence">HRDG_TRAINING_CIR_MASTER_SEQ</param>
			</generator>
		</id>
		<property name="circulationDate" column="cir_date" />
		<property name="departmentId" column="department_id" />
		<property name="nominationStartDate" column="nom_start_date" />
		<property name="nominationEndDate" column="nom_end_date" />
		<property name="organizer" column="organizer_sfid" />
		<property name="courseId" column="course_id" />
		<property name="venueId" column="venue_id" />
		<property name="durationId" column="course_duration_id" />
		<property name="brochure" column="brochure" />
		<property name="ionId" column="ion_id" />
		
		
		<property name="status" column="status" />
		<property name="creationDate" column="creation_Date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />


	</class>
</hibernate-mapping>

