<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.hrdg.training.beans.dto.CourseContentDTO" table="HRDG_COURSE_CONTENT_MASTER">
		<id name="id" column="course_content_id">
			<generator class="sequence">
				<param name="sequence">HRDG_COURSE_CONTENT_MASTER_SEQ</param>
			</generator>
		</id>
		<property name="courseId" column="course_id" />
		<property name="courseContent" column="course_content" />
		
	</class>
</hibernate-mapping>
