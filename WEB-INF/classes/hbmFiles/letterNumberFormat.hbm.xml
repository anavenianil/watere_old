<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.beans.letterNumberFormat.LetterNumberFormatDTO" table="LETTER_NUMBER_FORMAT_MASTER">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">LETTER_NUMBER_FORMAT_MAS_SEQ</param>
			</generator>
		</id>
		<property name="deptNum" column="DEPARTMENT_ID" />
		<property name="topic" column="TOPIC" />
		<property name="shortForm" column="SHORT_FORM" />
		<property name="serialNum" column="SERIAL_NUMBER" />
		<property name="yearType" column="YEAR_TYPE" />
		<property name="runningNum" column="RUNNING_NUMBER" />
		<property name="fileTypeId" column="FILE_TYPE_ID" />
		
		
		<property name="status" column="status" />
		<property name="creationDate" column="creation_Date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />


	</class>
</hibernate-mapping>
