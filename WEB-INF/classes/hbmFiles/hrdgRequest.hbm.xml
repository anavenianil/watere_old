<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.hrdg.training.request.beans.dto.HrdgTxnRequestDTO" table="HRDG_REQUEST_MASTER">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">HRDG_REQUEST_MASTER_SEQ</param>
			</generator>
		</id>
		<property name="requestId" column="request_id" />
		<property name="txnType" column="request_type" />
		<property name="requestStatus" column="request_status" />
		<property name="boardId" column="board_id" />
		<property name="ipAddress" column="ip_address" />
		<property name="mdbApprovedIonId" column="MDB_APPROVED_ION_ID" />
		<property name="adApprovedIonId" column="ad_approved_ion_id" />
		<property name="inistituteIonId" column="inistitute_ion_id" />
		
		
		
		<property name="status" column="status" />
		<property name="creationDate" column="creation_Date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />


	</class>
</hibernate-mapping>
