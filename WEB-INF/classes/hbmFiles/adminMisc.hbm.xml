<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.beans.misc.AdminMisc" table="ADMIN_MISC_DETAILS">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">ADMIN_MISC_DETAILS_SEQ</param>
			</generator>
		</id>
		<property name="sfID" column="SFID" />
		<property name="letterDt" column="LETTER_DATE" />			
		<property name="letterNo" column="LETTER_NO" />
		<property name="purpose" column="PURPOSE"/>
		<property name="type" column="MISC_TYPE"/>
		
	</class>
</hibernate-mapping>









