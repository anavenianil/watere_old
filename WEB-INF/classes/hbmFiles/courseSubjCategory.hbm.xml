<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.hrdg.training.beans.dto.CourseSubjCategoryDTO" table="HRDG_COURSE_SUBJ_CAT_MASTER">
		<id name="id" column="course_subject_cat_id">
			<generator class="sequence">
				<param name="sequence">HRDG_COURSE_SUBJ_CAT_MAS_SEQ</param>
			</generator>
		</id>
		<property name="name" column="course_subject_cat_name" />
		<property name="description" column="course_subject_cat_desc" />
		<property name="categoryId" column="category_id"/>
		<property name="status" column="status" />
		<property name="creationDate" column="creation_Date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />


	</class>
</hibernate-mapping>
