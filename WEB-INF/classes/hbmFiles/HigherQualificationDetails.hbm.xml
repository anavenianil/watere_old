<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.callippus.web.beans.higherQualification.dto.HQDetailsDTO" table="HIGHER_QUALIFICATION_DETAILS">
	<id name="id" column="id">
		<generator class="sequence">
				<param name="sequence">HIGHER_QUALIFICATION_DET_SEQ</param>
			</generator>
	</id>
	<property name="sfID" column="SFID"/>
	<property name="fromDate" column="FROM_DATE"/>
	<property name="course" column="COURSE"/>
	<property name="status" column="STATUS"/>
	<property name="dischargeOfDuties" column="DISCHARGE_OF_DUTIES"/>
	<property name="labWork" column="USEFUL_FOR_LAB"/>
	<property name="toDate" column="TO_DATE"/>
</class>
</hibernate-mapping>