<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.hrdg.training.beans.dto.TrainingInistituteDTO" table="HRDG_TRAINING_INIST_MASTER">
		<id name="id" column="training_inistitute_id">
			<generator class="sequence">
				<param name="sequence">HRDG_TRAINING_INIST_MASTER_SEQ</param>
			</generator>
		</id>
		<property name="name" column="training_inistitute_name" />
		<property name="shortName" column="training_institute_short_name" />
		<property name="trainingTypeId" column="training_type_id"/>
		<property name="trainingRegionId" column="region_id"/>
		<property name="status" column="status" />
		<property name="creationDate" column="creation_Date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />


	</class>
</hibernate-mapping>
