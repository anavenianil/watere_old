<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.beans.dto.TypeDetailsDTO" table="DOPART_II_TYPE_MASTER">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">DOPART_II_TYPE_MASTER_SEQ</param>
			</generator>
		</id>		
		<property name="name" column="NAME"/>
		<property name="prefixName" column="PREFIX"/>
		<property name="status" column="status" />	
		<property name="delimeter" column="delimeter" />		
		<property name="lastModifiedDate" column="LAST_MODIFIED_DATE" update="false" />
		<property name="creationDate" column="creation_date" update="false"/>	
		<property name="description" column="DESCRIPTION"/>	
	</class>
</hibernate-mapping>