<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.beans.dto.WorkflowDynamicTablesDTO" table="WORKFLOW_DYNAMIC_TABLES">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">WORKFLOW_DYNAMIC_TABLES_SEQ</param>
			</generator>
		</id>
		<property name="name" column="NAME" type="java.lang.String"/>
		<property name="status" column="STATUS" type="int"/>
		<property name="tableDTO" column="TABLE_DTO" type="java.lang.String"/>
		<property name="tableName" column="TABLE_NAME" type="java.lang.String"/>
	</class>
</hibernate-mapping>