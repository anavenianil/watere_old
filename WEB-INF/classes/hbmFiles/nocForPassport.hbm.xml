<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.beans.passport.PassportApplicationDTO" table="PASSPORT_APPLICATION">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">PASSPORT_APPLICATION_SEQ</param>
			</generator>
		</id>
		<property name="sfID" column="SFID" />
		<property name="letterDate" column="LETTER_DATE" />			
		<property name="letterDesc" column="LETTER_NO" />
		<property name="adminNoteNo" column="ADMIN_NOTE_NO"/>
		<property name="adminNoteDt" column="ADMIN_NOTE_DT"/>
		<property name="receivedDt" column="RECEIVED_DATE" />
		<property name="status" column="STATUS" />
		<property name="passportAppType" column="PASSPORT_TYPE" />
		
		<property name="idCertificateNo" column="ID_CERTIFICATE_NO" />
		<property name="idCertificateDt" column="ID_CERTIFICATE_DATE" />
		
	</class>
</hibernate-mapping>









