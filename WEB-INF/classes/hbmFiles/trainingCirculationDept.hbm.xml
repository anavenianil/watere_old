<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.hrdg.training.beans.dto.TrainingCirculationDeptDTO"
		table="HRDG_CIRCULATION_DEPARTMENTS">
	   <composite-id>
		<key-property name="circulationId" column="cir_id" />
		<key-property name="departmentId" column="department_id" />
		</composite-id>
		<property name="status" column="status" />
		<property name="creationDate" column="creation_Date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />
		
	</class>
</hibernate-mapping>
