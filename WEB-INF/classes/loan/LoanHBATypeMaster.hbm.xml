<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.callippus.web.loan.beans.dto.LoanHBATypeMasterDTO" table="loan_hba_type_master" >
	<id name="id" column="id">
		<generator class="sequence">
				<param name="sequence">LOAN_HBA_TYPE_MASTER_SEQ</param>
			</generator>
	</id>
	<property name="hbaAdvanceType" column="hba_advance_type" type="java.lang.String" />
	<property name="status" column="status" type="int" />
</class>
</hibernate-mapping>
