<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.beans.telephone.TelephoneEmpEligibiltyDetailsDTO" table="TELE_EMP_ELIGIBILTY_DETAILS">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">TELE_EMP_ELIG_DETAILS_SEQ</param>
			</generator>
		</id>
		<property name="designationId" column="DESIGNATION_ID" />
		<property name="sfid" column="SFID" />
		<property name="status" column="STATUS" />
	</class>
</hibernate-mapping>


