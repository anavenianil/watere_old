<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.beans.telephone.TelephoneBillEligibleDTO" table="TELEPHONE_ELIGIBILITY_DETAILS">
		<id name="id" column="id"><generator class="increment"/></id>
		<property name="designationId" column="DESIGNATION_ID" />
		<property name="internetFlag" column="INTERNET_FLAG" />
		<property name="amount" column="AMOUNT"/>
		<property name="serviceTax" column="SERVICE_TAX" />
		<property name="sfid" column="SFID" />
		<property name="totAmount" column="TOTAL_AMOUNT" />
	</class>
</hibernate-mapping>