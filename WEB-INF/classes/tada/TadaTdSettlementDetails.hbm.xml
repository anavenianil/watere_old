<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.tada.dto.TadaTdSettlementDTO" table="tada_td_settlement_details">
		<id name="id" column="id">
		<generator class="increment" />
		</id>
		<property name="requestType" column="REQUEST_TYPE" />
		<property name="requestID" column="REQUEST_ID" />
		<property name="refRequestId" column="REF_REQUEST_ID" />
		<property name="amountClaimed" column="AMOUNT_CLAIMED" />
		<property name="amountClaimedAftRes" column="AMOUNT_CLAIMED_AFT_RES" />
		<property name="appliedBy" column="APPLIED_BY" />
		<property name="userRemarks" column="REMARKS" />
	</class>
</hibernate-mapping>