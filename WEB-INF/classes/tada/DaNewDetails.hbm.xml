<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.tada.dto.TadaDaNewDetailsDTO" table="tada_da_new_details_master">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">TADA_DA_NEW_DETAILS_MASTER_SEQ</param>
			</generator>
		</id>
		<property name="gradePay" column="grade_pay" />
		<property name="accommodationBill" column="accommodation_bill" />
		<property name="foodBill" column="food_bill" />
		<property name="milageAllw" column="milage_allw" />
		<property name="travelBy" column="travel_by" />
		<property name="distance" column="distance" />
		<property name="amount" column="amount" />
		<property name="status" column="status" />
		<property name="creationDate" column="creation_date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />
	</class>
</hibernate-mapping>