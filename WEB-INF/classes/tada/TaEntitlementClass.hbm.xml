<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.tada.dto.TaEntitleClassDTO" table="tada_ta_entitleclass">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">TADA_TA_ENTITLECLASS_SEQ</param>
			</generator>
		</id>
		<property name="gradePay" column="grade_pay" />
		<property name="entitleTypeId" column="entitle_type_id" />
		<property name="entitleClassId" column="entitle_class_id" />
		<property name="status" column="status" />
		<property name="creationDate" column="creation_date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />
	</class>
</hibernate-mapping>