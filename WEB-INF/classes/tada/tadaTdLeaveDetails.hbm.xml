<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.tada.dto.TadaTdLeaveDetailsDTO" table="tada_td_leave_details">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">TADA_TD_LEAVE_DETAILS_SEQ</param>
			</generator>
		</id>
		<property name="refRequestId" column="ref_request_id" />
		<property name="leaveRequestId" column="leave_request_id" />
	</class>
</hibernate-mapping>