<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.tada.dto.TadaTdDaNewAccDetailsDTO" table="tada_td_da_new_acc_details">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">TADA_TD_DA_NEW_ACC_SEQ</param>
			</generator>
		</id>
		<property name="settlementRequestId" column="settlement_request_id" />
		<property name="fromDate" column="from_date" />
		<property name="toDate" column="to_date" />
		<property name="inTime" column="in_time" />
		<property name="outTime" column="out_time" />
		<property name="accAmount" column="acc_amount" />
		<property name="calAccAmount" column="cal_acc_amount" />
		<property name="claimedAmount" column="claimed_amount" />
		<property name="accAmountAftRes" column="acc_amount_aft_res" />
		<property name="claimedAmountAftRes" column="claimed_amount_aft_res"/>
		<property name="type" column="request_type"/>
	</class>
</hibernate-mapping>