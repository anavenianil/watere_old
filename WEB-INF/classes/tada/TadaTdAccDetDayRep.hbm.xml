<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.tada.dto.TadaTdAccDetDayRepDTO" table="tada_td_acc_det_day_rep">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">TADA_TD_ACC_DET_SEQ</param>
			</generator>
		</id>
		<property name="settlementRequestId" column="settlement_request_id" />
		<property name="representationDate" column="representation_date" />
		<property name="claimedAmount" column="claimed_amount" />
		<property name="eligibleAmount" column="eligible_amount" />
		<property name="admisAmount" column="admis_amount" />
	</class>
</hibernate-mapping>