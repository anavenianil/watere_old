<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.tada.dto.CityTypeDTO" table="tada_city_type">
		<id name="id1" column="id1">
			<generator class="sequence">
				<param name="sequence">TADA_CITY_TYPE_SEQ</param>
			</generator>
		</id>
		<property name="id" column="id" />
		<property name="cityClass" column="city_type" />
		<property name="cityName" column="city_name" />
		<property name="status" column="status" />
		<property name="creationDate" column="creation_date" update="false"/>
		<property name="createdBy" column="created_by" update="false"/>
		<property name="lastModifiedDate" column="last_modified_date" />
		<property name="lastModifiedBy" column="last_modified_by" />
	</class>
</hibernate-mapping>