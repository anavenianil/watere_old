<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<hibernate-mapping>
		<class name="com.callippus.web.hindi.dto.HindiEmployeeDTO" table="HINDI_EMPLOYEE_MASTER">
				<id name="sfid"  column="SFID">
					<generator class="sequence">
				<param name="sequence">HINDI_EMPLOYEE_MASTER_SEQ</param>
			</generator>
				</id>	
				<property name="nonEligibleExamId" column="NON_ELIGIBLE_EXAMID" />								
				<!-- <property name="mothertongue" column="MOTHERTONGUE" /> -->
				<property name="remarks" column="REMARKS" />																
				<property name="status" column="STATUS" />
		</class> 
	</hibernate-mapping>