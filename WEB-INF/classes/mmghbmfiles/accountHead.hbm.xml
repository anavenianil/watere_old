<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.mmg.cashbuildup.dto.AccountHeadDTO" table="MMG_B_ACCOUNT_HEAD_MASTER">
		<id name="accId" column="id">
			<generator class="sequence">
				<param name="sequence">MMG_B_ACCOUNT_HEAD_MASTER_SEQ</param>
			</generator>
		</id>
		<property name="accountHeadNumber" column="ACCOUNT_HEAD_NUMBER" />
		<property name="qtyHeld" column="QTY_HELD" />
		<property name="qtyRequired" column="QTY_REQUIRED" />
		<property name="description" column="ACCOUNT_DESCRIPTION" />
		<property name="allottedFund" column="ALLOTTED_FUND" />
		<property name="year" column="YEAR" />
		<property name="fundTypeId" column="FUND_TYPE_ID" />
		<property name="departmentId" column="DEPARTMENT_ID" />
		<property name="consumedFund" column="CONSUMED_FUND" />
		<property name="commitedFund" column="COMMITTED_FUND" />
		<property name="status" column="STATUS" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="lastModifiedDate" column="LAST_MODIFIED_DATE" />
		
	</class>
</hibernate-mapping>