<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.mmg.cashbuildup.dto.DemandItemDetailsDTO" table="MMG_B_DEMAND_ITEM_DETAILS">
		<id name="id" column="ID" >
			<generator class="sequence">
				<param name="sequence">MMG_B_DEMAND_ITEM_DETAILS_SEQ</param>
			</generator>
		</id>		
		<property name="demandNo" column="DEMAND_NO" />
		<property name="materialCode" column="MATERIAL_CODE"/>
		<property name="amountTypeId" column="AMOUNT_TYPE" />
		<property name="qty" column="QTY" />
		<property name="unitRate" column="UNIT_RATE" />
		<property name="uomConvert" column="UOM" />
		<property name="taxTypeId" column="TAX_TYPE_ID" />
		<property name="taxAmount" column="TAX_AMOUNT" />		
		<property name="status" column="STATUS" />
		<property name="stageId" column="STAGE_ID" />
		<property name="changedBy" column="CHANGED_BY" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="lastModifiedDate" column="LAST_MODIFIED_DATE" />
    </class>
</hibernate-mapping>