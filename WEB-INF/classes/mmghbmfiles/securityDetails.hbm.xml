<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.mmg.cashbuildup.dto.SecurityDetailsDTO" table="MMG_B_SECURITY_DETAILS">
		<id name="id" column="ID" type="int">
			<generator class="sequence">
				<param name="sequence">MMG_B_SECURITY_DETAILS_SEQ</param>
			</generator>
		</id>
		<property name="memoNo" column="MEMO_NO" />
		<property name="memoDate" column="MEMO_DATE" />
		<property name="securityMemoDate" column="MEMO_DATE" insert="false" update="false"/>
		<property name="demandNo" column="DEMAND_NO" />
		<property name="supplierId" column="SUPPLIER_ID" />
		<property name="materialCode" column="MATERIAL_CODE" />
		<property name="qty" column="QTY" />
		<property name="uom" column="UOM" />
		<property name="verifiedBy" column="VERIFIED_BY" />
		<property name="status" column="STATUS" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="lastModifiedDate" column="LAST_MODIFIED_DATE" />
   </class>
</hibernate-mapping>