<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.mmg.cashbuildup.dto.ItemSubCategoryDTO" table="MMG_B_ITEM_SUB_CATEGORY_MASTER">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">MMG_B_ITEM_SUB_CAT_MASTER_SEQ</param>
			</generator>
		</id>
		<property name="name" column="SUB_CATEGORY_NAME" />
		<property name="categoryId" column="CATEGORY_ID" />
		<property name="subCategoryCode" column="SUB_CODE" />
		<property name="status" column="STATUS" />
		<property name="description" column="DESCRIPTION" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="lastModifiedDate" column="LAST_MODIFIED_DATE" />
	</class>
</hibernate-mapping>