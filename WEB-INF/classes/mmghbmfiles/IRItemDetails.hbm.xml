<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.mmg.cashbuildup.dto.IRItemDetailsDTO" table="MMG_B_IR_ITEM_DETAILS">
		<id name="id" column="ID" type="int">
			<generator class="sequence">
				<param name="sequence">MMG_B_IR_ITEM_DETAILS_SEQ</param>
			</generator>
		</id>		
		<property name="voucherNo" column="VOUCHER_NO" />
		<property name="qty" column="QTY" />
		<property name="materialCode" column="MATERIAL_CODE" />
		<property name="unitRate" column="RATE"/>
		<property name="taxTypeId" column="TAX_TYPE_ID" />
		<property name="taxAmount" column="TAX_AMOUNT" />		
		<property name="status" column="STATUS" />
		<property name="supplierId" column="SUPPLIER_ID" />
		<property name="uom" column="UOM" />
		<property name="memoNo" column="MEMO_NO" />
		<property name="memoDate" column="MEMO_DATE"/>
		<property name="changedBy" column="CHANGED_BY" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="lastModifiedDate" column="LAST_MODIFIED_DATE" />
    </class>
</hibernate-mapping>