<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.mmg.cashbuildup.dto.InventoryMasterDTO" table="MMG_B_INVENTORY_HOLDER">
		<id name="invId" column="id">
			<generator class="sequence">
				<param name="sequence">MMG_B_INVENTORY_HOLDER_SEQ</param>
			</generator>
		</id>
		<property name="inventoryNo" column="INVENTORY_NO" />
		<property name="sfid" column="SFID" />
		<property name="orgRoleId" column="ORG_ROLE_ID" />
		<property name="inventoryHolderType" column="INVENTORY_HOLDER_TYPE" />
		<property name="projectName" column="PROJECT_ID" />
		<property name="status" column="STATUS" />
		<property name="creationDate" column="CREATION_DATE"/>
		<property name="lastModifiedDate" column="LAST_MODIFIED_DATE" />
	</class>
</hibernate-mapping>