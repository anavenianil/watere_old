<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.callippus.web.paybill.dto.PayBillCcsDTO" table="PAY_CCS_DETAILS" >
		<id name="id" type="int" column="id">
			<generator class="sequence">
				<param name="sequence">PAY_CCS_DETAILS_SEQ</param>
			</generator>
		</id>
		<property name="msno" column="MSNO" />
		<property name="sfid" column="SFID"  />
		<property name="name" column="NAME"  />
		<property name="thrift" column="THRIFT" />
		<property name="loan" column="LOANINS"  />
		<property name="loanInst" column="INST"  />
		<property name="loanBal" column="LOANBL" />
		<property name="rd" column="RD"  />
		<property name="eml" column="EMLINS"  />
		<property name="emlInst" column="EMLINT" />
		<property name="emlBal" column="EMLBL"  />
		<property name="misc" column="MISC"  />
		<property name="total" column="TOTAL"  />
		<property name="status" column="STATUS"  />
		<property name="creationDate" column="CREATION_DATE"  update="false"/>
		<property name="createdBy" column="CREATED_BY"  update="false"/>
		<property name="lastModifiedDate" column="MODIFIED_DATE"  />
		<property name="lastModifiedBy" column="MODIFIED_BY"  />
		
	</class>
</hibernate-mapping>