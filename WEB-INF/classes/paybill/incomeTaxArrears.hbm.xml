		<property name="tptDrawn" column="TPT_DRAWN"  />
		<property name="tptDiff" column="TPT_DIFF"  />
		<property name="cpf" column="CPF"  />
		<property name="itRec" column="IT_REC"  />
		<property name="sfid" column="SFID"  />
		<property name="month" column="MONTH"  />	
		<property name="dateFrom" column="DATE_FROM"  />	
		<property name="dateTo" column="DATE_TO"  />		
		<property name="creationDate" column="CREATION_DATE"/>
		<property name="createdBy" column="CREATED_BY"/>
		<property name="lastModifiedDate" column="LAST_MODIFIED_DATE"  />
		<property name="lastModifiedBy" column="LAST_MODIFIED_BY"  />
		<property name="status" column="STATUS" />
</class>
</hibernate-mapping>