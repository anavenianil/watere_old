<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.callippus.web.paybill.dto.PayBillGroupMasterDTO" table="GROUP_MASTER" lazy="false">
		<id name="pk" column="ID" type="int">
			<generator class="sequence">
				<param name="sequence">GROUP_MASTER_SEQ</param>
			</generator>
		</id>
		<property name="name" column="NAME" type="string" />
		<property name="status" column="STATUS" type="int" />
		<property name="creationDate" column="CREATION_DATE" type="date" />
		<property name="lastModifiedDate" column="LAST_MODIFIED_DATE" type="date" />
	</class>
</hibernate-mapping>