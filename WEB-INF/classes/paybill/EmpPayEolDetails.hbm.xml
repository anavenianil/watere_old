<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.callippus.web.paybill.dto.PayEolDTO" table="pay_eol_mapping">	
		<id name="id"  type="int" column="id">
			<generator class="sequence">
				<param name="sequence">PAY_EOL_MAPPING_SEQ</param>
			</generator>
		</id>
		<property name="sfid" column="sfid"  not-null="true" />
		<property name="requestID" column="request_id"  />
		<property name="runMonth" column="run_month"  />
		<property name="status" column="status"  />
		<property name="createdBy" column="created_by"  />
		<property name="creationDate" column="creation_date"  />
		<property name="lastModifiedBy" column="modified_by"  />
		<property name="lastModifiedDate" column="modified_date"  />
		
	</class>
</hibernate-mapping>
