<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.callippus.web.beans.MT.MTArticleDetailsDTO"
		table="MT_ARTICLE_DETAILS">
		<id name="id" type="int" column="ID">
			<generator class="sequence">
				<param name="sequence">MT_ARTICLE_DETAILS_SEQ</param>
			</generator>
		</id>
		<property name="referenceID" column="REFERENCE_ID" />
		<property name="type" column="TYPE" />
		<property name="length" column="LENGTH" />
		<property name="breadth" column="BREADTH" />
		<property name="height" column="HEIGHT" />
		<property name="weight" column="WEIGHT" />
		<property name="quantity" column="QUANTITY" />
		<property name="journeyTypeFlag" column="JOURNEY_TYPE_FLAG" />
		<property name="status" column="STATUS" />
	</class>
</hibernate-mapping>

		